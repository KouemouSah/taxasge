        "messaging_sender_id": "MANAGED_BY_FIREBASE_CONFIG_JS"
      },
      "supabase": {
        "project_ref": "taxasge-prod",
        "url_ref": "SUPABASE_URL",
        "service_role_key_ref": "SUPABASE_SERVICE_ROLE_KEY", 
        "anon_key_ref": "SUPABASE_ANON_KEY",
        "region": "eu-west-3",
        "database_name": "taxasge-db",
        "schema": "public"
      },
	  "database": {
