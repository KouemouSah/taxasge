{
  "name": "@taxasge/mobile",
  "version": "1.0.0",
  "description": "TaxasGE Mobile - Application de Gestion Fiscale pour la Guinée Équatoriale",
  "private": true,
  "main": "index.js",
  "author": {
    "name": "KOUEMOU SAH Jean Emac",
    "email": "kouemou.sah@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/KouemouSah/taxasge#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/KouemouSah/taxasge.git",
    "directory": "packages/mobile"
  },
  "bugs": {
    "url": "https://github.com/KouemouSah/taxasge/issues"
  },
  "keywords": [
    "react-native",
    "mobile",
    "tax-management",
    "guinea-ecuatorial",
    "firebase",
    "supabase",
    "ai-chatbot",
    "offline-first"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "build:android": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:ios": "react-native run-ios --configuration Release",
    "build:ios:release": "xcodebuild -workspace ios/TaxasGE.xcworkspace -scheme TaxasGE -configuration Release -destination generic/platform=iOS -archivePath ios/build/TaxasGE.xcarchive archive",
    "clean": "react-native clean-project-auto",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "clean:node": "rm -rf node_modules && yarn install",
    "clean:metro": "npx react-native start --reset-cache",
    "postinstall": "cd ios && pod install --repo-update",
    "pods": "cd ios && pod install",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "reverse": "adb reverse tcp:8081 tcp:8081",
    "precommit": "lint-staged",
    "prepare": "cd ../.. && husky install packages/mobile/.husky",
    "doctor": "react-native doctor",
    "info": "react-native info"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.0",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-firebase/analytics": "^18.6.1",
    "@react-native-firebase/app": "^18.6.1",
    "@react-native-firebase/app-distribution": "^18.6.1",
    "@react-native-firebase/auth": "^18.6.1",
    "@react-native-firebase/crashlytics": "^18.6.1",
    "@react-native-firebase/firestore": "^18.6.1",
    "@react-native-firebase/messaging": "^18.6.1",
    "@react-native-firebase/storage": "^18.6.1",
    "@react-navigation/bottom-tabs": "^6.5.9",
    "@react-navigation/drawer": "^6.6.4",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/stack": "^6.3.18",
    "@reduxjs/toolkit": "^1.9.7",
    "@supabase/supabase-js": "^2.38.0",
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow/tfjs-react-native": "^0.8.0",
    "@react-native-community/datetimepicker": "^7.6.3",
    "axios": "^1.5.1",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "formik": "^2.4.5",
    "i18next": "^23.6.0",
    "i18next-react-native-language-detector": "^1.0.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "react-i18next": "^13.3.1",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "react-native-action-button": "^2.8.5",
    "react-native-animatable": "^1.3.3",
    "react-native-biometrics": "^3.0.1",
    "react-native-bootsplash": "^5.4.0",
    "react-native-calendars": "^1.1300.0",
    "react-native-config": "^1.5.1",
    "react-native-date-picker": "^4.3.3",
    "react-native-device-info": "^10.11.0",
    "react-native-document-picker": "^9.1.1",
    "react-native-dotenv": "^3.4.9",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-image-crop-picker": "^0.40.3",
    "react-native-image-picker": "^7.0.3",
    "react-native-keychain": "^8.1.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-mmkv": "^2.10.2",
    "react-native-modal": "^13.0.1",
    "react-native-orientation-locker": "^1.6.0",
    "react-native-pdf": "^6.7.3",
    "react-native-permissions": "^3.10.1",
    "react-native-picker-select": "^8.1.0",
    "react-native-print": "^0.8.0",
    "react-native-progress": "^5.0.1",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.25.0",
    "react-native-share": "^9.4.1",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-super-grid": "^5.0.1",
    "react-native-svg": "^13.14.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-webview": "^13.6.3",
    "react-native-flash-message": "^0.4.2",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.22.5",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.0",
    "@react-native/metro-config": "^0.73.0",
    "@react-native/typescript-config": "^0.73.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "detox": "^20.13.4",
    "eslint": "^8.19.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^15.0.2",
    "metro-react-native-babel-preset": "^0.76.8",
    "patch-package": "^8.0.0",
    "prettier": "^2.4.1",
    "react-native-bundle-visualizer": "^3.1.3",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "@babel/plugin-transform-nullish-coalescing-operator",
    "rimraf": "^4.4.1",
    "glob": "^9.3.5",
    "core-js": "^3.30.0",
    "@xmldom/xmldom": "^0.8.6"
  },
  "react-native": {
    "zlib": "react-native-level-fs",
    "path": "path-browserify",
    "crypto": "react-native-crypto"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-vector-icons|react-native-super-grid|react-native-reanimated|react-native-gesture-handler|@react-navigation|@react-native-firebase|@tensorflow|react-native-calendars|react-native-element-dropdown)/)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/**/index.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json}": [
      "prettier --write"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "android.emu.debug": {
        "type": "android.emulator",
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "device": {
          "avdName": "Pixel_3a_API_30_x86"
        }
      },
      "android.emu.release": {
        "type": "android.emulator",
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "device": {
          "avdName": "Pixel_3a_API_30_x86"
        }
      },
      "ios.sim.debug": {
        "type": "ios.simulator",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/TaxasGE.app",
        "device": {
          "type": "iPhone 14"
        }
      }
    }
  },
  "metro": {
    "resolver": {
      "assetExts": [
        "png",
        "jpg",
        "jpeg",
        "gif",
        "mp4",
        "webm",
        "wav",
        "mp3",
        "m4a",
        "aac",
        "tflite",
        "json"
      ],
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "json"
      ]
    }
  },
  "config": {
    "project": {
      "name": "TaxasGE",
      "displayName": "TaxasGE - Gestion Fiscale",
      "bundleId": "com.taxasge.app",
      "version": {
        "name": "1.0.0",
        "code": 1
      },
      "firebase": {
        "development": "taxasge-dev",
        "production": "taxasge-prod"
      },
      "features": {
        "ai_chatbot": true,
        "offline_mode": true,
        "biometric_auth": true,
        "push_notifications": true,
        "crash_reporting": true,
        "analytics": true,
        "multi_language": true
      }
    },
    "build": {
      "android": {
        "minSdkVersion": 21,
        "targetSdkVersion": 34,
        "compileSdkVersion": 34
      },
      "ios": {
        "deploymentTarget": "12.4",
        "bundleShortVersionString": "1.0",
        "bundleVersion": "1"
      }
    },
    "performance": {
      "hermes": true,
      "newArchitecture": false,
      "flipper": false
    }
  }
}